@{
    ViewData["Title"] = "CincoData";
    Layout = null;
    //Layout = "~/Views/Shared/_Layout.cshtml";
}

<link rel="stylesheet" href="@Url.Content("~/css/CincoStyles.css")" />

@if (Model != null && Model.Count > 0)
{
    <div class="body">
        <div id="informacion">
            <div class="datos">
                <h2>@Model[0].cedula</h2>
                <h2 id="nombre">@Model[0].nombre</h2>
            </div>
            <div class="infoDatos">
                <div><strong>Hora:</strong> @DateTime.Now.ToString("h:mm tt") </div>
                <div><strong>Fecha:</strong> @DateTime.Now.ToString("dddd, dd/MM/yyyy h:mm tt")</div>
            </div>
        </div>

        <table class="table">
            <tr><div>416542 Aportes de pastor</div></tr>
            <tr>
                <th>Comprobante</th>
                <th>Num Comprobante</th>
                <th>Fecha</th>
                <th>Observacion</th>
                <th>Debitos</th>
                <th>Creditos</th>
            </tr>
            @foreach (var item in Model)
            {

                <tr>
                    <td>@item.codComprobante</td>
                    <td>@item.numComprobante</td>
                    <td>@item.fecha</td>
                    <td>@item.observacion</td>
                    <td>@item.valorDebitos</td>
                    <td>@item.valorCreditos</td>

                </tr>
            }
        </table>

    </div>


}

